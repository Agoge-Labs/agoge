# Agoge Environment Variables
# Copy this file to .env.local and fill in values after deploying contracts

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=demo-project-id

# Chain Configuration
NEXT_PUBLIC_CHAIN_ID=84532

# Deployed Contract Addresses (Base Sepolia)
# Fill these after deploying via Remix IDE
NEXT_PUBLIC_INTENT_ROUTER_ADDRESS=
NEXT_PUBLIC_AGENT_REGISTRY_ADDRESS=
NEXT_PUBLIC_REPUTATION_MANAGER_ADDRESS=
NEXT_PUBLIC_PRICE_ORACLE_ADDRESS=
NEXT_PUBLIC_MOCK_USDC_ADDRESS=

# CoinGecko API
# Get a free demo API key from https://www.coingecko.com/en/api
COINGECKO_API_KEY=

# Cron Secret (for agent automation, optional)
CRON_SECRET=agoge-cron-secret-2026
